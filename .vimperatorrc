"3.6 (created: 2012/12/24 13:05:56)

highlight CmdLine color: #aaa; background-color: #222;
highlight CmdOutput color: #aaa; background-color: #222;
highlight Normal color: #999; background-color: #222;
highlight StatusLine color: #999; background-color: #222;
highlight Hint color: #afd700; background-color: #333;
highlight HintElem color: #5fd7ff; background-color: #333;
highlight HintActive background-color: #5fd7ff; color: #333;
highlight Function color: #afd700;
highlight CompTitle font-weight: bold; background: -moz-linear-gradient(19% 75% 90deg, #444, #444, #444 100%);
highlight CompTitle>* color: #aaa;
highlight CompItem[selected] background: #999; color: #222; box-shadow: 0px 0px 1px #CC0;
highlight InfoMsg background: #333;
highlight LineNr background: #444;
highlight ModeMsg background: #444; color: #afd700;
highlight MoreMsg background: #444; color: #afd700;
highlight Question background: #333; color: #aaa;
highlight Title background: #222; color: #9f0;
highlight WarningMsg background: #222; color: #f00;
source! ~/.vimperatorrc.local

nmap h :back<CR>
nmap l :forward<CR>
