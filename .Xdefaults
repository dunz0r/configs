! URxvt
URxvt*font:xft:Inconsolata:size=9:antialias=true
URxvt*boldFont:xft:Inconsolata:bold:size=9:antialias=true
URxvt*letterSpace: -1
URxvt*loginShell:       true
URxvt*saveLines:        5500
URxvt*cutchars:         `"()'*<>[]{|}
/* places a hardcopy in home dir when print is pressed */
URxvt*print-pipe: cat > $(TMPDIR=$HOME mktemp urxvt.XXXXX)

URxvt*perl-ext:      default,url-select
URxvt*keysym.C-M-u:    perl:url-select:select_next
URxvt*urlLauncher:   firefox -new-tab
URxvt*underlineURLs: true

URxvt*depth: 32
URxvt*background: #111
URxvt*foreground: #aaa

URxvt*transparent:        true
URxvt*cursorBlink:        true
URxvt*cursorOnTime:       300
URxvt*cursorOffTime:      140
URxvt*shading:            10
!URxvt*fading:             40
URxvt*scrollBar:          False

URxvt*colorBD:            lightyellow
URxvt*colorUL:            grey
URxvt*cursorColor: #999

Xft*dpi:	96
Xft*antialias:  true
Xft*rgba:       rgb
Xft*hinting:    true
Xft*hintstyle:  hintlight
Xft*lcdfilter:  lcddefault

! this line makes so that only the SSH-sessions become "Urgent" on bell
SSH*urgentOnBell: true

! XTerm
XTerm*Background: black
XTerm*Foreground: grey90
XTerm*LoginShell: true
XTerm*CursorColor: rgb:99/99/00
XTerm*printerAutoClose: true

! XLock
XLock.invalidCapsLock: Invalid login.
XLock.mode: matrix
XLock.info:  
XLock.program: fortune -ao
XLock.username:
XLock.password:
XLock.erasetime: 3
XLock.mono: true

! Which theme to use
#include "/home/gabriel/.xdefaults-colour/bleh-1.Xresources"
! EOF
