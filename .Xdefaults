! URxvt
URxvt*font: xft:Inconsolata:size=8:antialias=true
URxvt*boldFont: xft:Inconsolata:bold:size=8:antialias=true
URxvt*letterSpace: -1
URxvt*loginShell:       true
URxvt*saveLines:        5500
URxvt*cutchars:         `"()'*<>[]{|}
/* places a hardcopy in home dir when print is pressed */
URxvt*print-pipe: cat > $(TMPDIR=$HOME mktemp urxvt.XXXXX)

URxvt*perl-ext:      default,url-select
URxvt*keysym.C-M-u:    perl:url-select:select_next
URxvt*urlLauncher:   firefox -new-tab
URxvt*underlineURLs: true

URxvt*depth: 32

URxvt*transparent:        true
URxvt*cursorBlink:        true
URxvt*cursorOnTime:       300
URxvt*cursorOffTime:      140
URxvt*shading:            10
!URxvt*fading:             40
URxvt*scrollBar:          False

URxvt*colorBD:            lightyellow
URxvt*colorUL:            green
URxvt*cursorColor: yellow

Xft*dpi:	96
Xft*rgba:	rgb
Xft*hinting:	true
Xft*hintstyle:	hintslight
XLock.mono:	true
XLock.info:	
XLock.username:	
XLock.program:	fortune -o
XLock.password:	
XLock.mode:	matrix
XLock.erasetime:	3
XLock.invalidCapsLock:	Invalid login.


#include "/home/gabriel/.xdefaults-colour/molokai"
